% ==============================================================================
% Sets
% ==============================================================================
STAFF = {Alice, Bob, Charlie};
ACTIVITIES = {a, b, c};
SKILLS = {teaching, logistics, programming, ci_cd, devops};

% ==============================================================================
% Parameters
% ==============================================================================

% available[s, a]: true if staff member 's' is available for activity 'a'
available = array2d(STAFF, ACTIVITIES, [
    % a,    b,    c
    true, true, false,  % Alice
    true, true, false,  % Bob
    true, true, true    % Charlie
]);

% minStaff[a]: The minimum number of staff required for activity 'a'
minStaff = [1, 2, 1];

% preference[s, a]: The preference value (float) of staff 's' for activity 'a'
preference = array2d(STAFF, ACTIVITIES, [
    % a,   b,   c
    3.0, 4.0, 3.0,  % Alice
    5.0, 3.0, 1.0,  % Bob
    3.0, 3.0, 2.0   % Charlie
]);

% staffSkills[s, k]: true if staff member 's' has skill 'k'
staffSkills = array2d(STAFF, SKILLS, [
    % teaching, logistics, programming, ci_cd, devops
    true,       true,      false,       false, false,  % Alice
    true,       false,     true,        false, false,  % Bob
    false,      false,     false,       true,  true   % Charlie
]);

% activitySkills[a, k]: true if activity 'a' requires skill 'k'
activitySkills = array2d(ACTIVITIES, SKILLS, [
    % teaching, logistics, programming, ci_cd, devops
    true,       false,     true,        false, false,  % a
    false,      true,      false,       false, false,  % b
    true,       false,     false,       true,  true   % c
]);

% experience[s]: The experience level (1-5) of staff member 's'
experience = [1, 3, 5];

% start[a]: The start time block for activity 'a'
start = [10, 11, 15];

% duration[a]: The duration in time blocks for activity 'a'
duration = [2, 1, 2];

% Weights for the objective function
w1 = 1.0;
w2 = 1.0;
w3 = 1.0;
w4 = 1.0;
